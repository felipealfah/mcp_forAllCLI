{
  "mcpServers": {
    "example-ai-server": {
      "command": "node",
      "args": ["/Users/felipefull/Documents/MCP_servers/servers/ai/example-server/server.js"],
      "env": {
        "API_KEY": "your_api_key_here",
        "API_URL": "https://api.example.com/v1"
      }
    },
    "git-server": {
      "command": "node",
      "args": ["/Users/felipefull/Documents/MCP_servers/servers/development/git-server/server.js"],
      "env": {
        "GITHUB_TOKEN": "your_github_token_here"
      }
    },
    "database-server": {
      "command": "python3",
      "args": ["/Users/felipefull/Documents/MCP_servers/servers/database/postgres-server/server.py"],
      "env": {
        "POSTGRES_HOST": "localhost",
        "POSTGRES_PORT": "5432",
        "POSTGRES_USER": "your_username",
        "POSTGRES_PASSWORD": "your_password",
        "POSTGRES_DB": "your_database"
      }
    }
  },
  "mcp": {
    "enabled": true,
    "autoStart": true,
    "logLevel": "info"
  }
}
